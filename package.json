{
  "name": "quotefast-ai-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "demo": "./demo.sh",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rm -rf .next out dist",
    "precommit": "npm run type-check && npm run lint && npm run test",
    "db:migrate": "echo 'Run the SQL migration in Supabase Dashboard: supabase/migrations/001_initial_schema.sql'",
    "db:reset": "echo 'Reset database by running the migration again'",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "export": "next build && next export",
    "netlify:build": "npm run build",
    "netlify:deploy": "netlify deploy --prod"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@react-email/components": "^0.5.6",
    "@react-email/render": "^1.3.2",
    "@stripe/stripe-js": "^8.0.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.0",
    "@vercel/sdk": "^1.13.8",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "inngest": "^3.44.2",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.294.0",
    "next": "14.0.0",
    "openai": "^6.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-email": "^4.3.0",
    "react-hook-form": "^7.65.0",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.2.1",
    "resend": "^6.1.2",
    "stripe": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  }
}
